<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ora.dao.HistoryDao">
    <!-- 查询所有备份记录 -->
    <select id="selectAllHistory" resultType="history">
        SELECT * FROM history;
    </select>
    <!-- 根据部门id查询备份记录 -->
    <select id="selectHistoryByDid" parameterType="Integer" resultType="history">
        SELECT * from history WHERE udepartment = #{udepartment}
    </select>
</mapper>